{
  "name": "site",
  "version": "0.1.0",
  "homepage": "https://www.itabits.com.br",
  "private": true,
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "1.1.1",
    "semantic-ui-css": "^2.3.0",
    "semantic-ui-react": "^0.78.3"
  },
  "devDependencies": {
    "gh-pages": "^1.1.0",
    "standard": "^11.0.1"
  },
  "standard": {
    "ignore": [
      "node_modules/**",
      "src/registerServiceWorker.js"
    ],
    "parser": "babel-eslint"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "predeploy": "npm run build && echo %npm_package_homepage% > ./build/CNAME && sed -i 's/https\\?:\\/\\///g' ./build/CNAME",
    "deploy": "gh-pages -d build"
  }
}
